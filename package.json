{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push --follow-tags"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@asany/tags-input",
  "author": "李茂峰",
  "repository": "https://github.com/limaofeng/asany-tags-input",
  "module": "dist/tags-input.esm.js",
  "size-limit": [
    {
      "path": "dist/tags-input.cjs.production.min.js",
      "limit": "100 KB"
    },
    {
      "path": "dist/tags-input.esm.js",
      "limit": "50 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@size-limit/preset-small-lib": "^5.0.2",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addons": "^6.3.6",
    "@storybook/react": "^6.3.6",
    "@types/lodash-es": "^4.17.4",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/react-is": "^17.0.2",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "conventional-changelog-cli": "^2.1.1",
    "cssnano": "^5.0.7",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest-fetch-mock": "^3.0.3",
    "node-sass": "^6.0.1",
    "pinst": "^2.1.6",
    "postcss": "^8.3.6",
    "postcss-modules": "^4.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup-plugin-postcss": "^4.0.0",
    "sass-loader": "^10.2.0",
    "size-limit": "^6.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react-use": "^17.3.2"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.1.1",
    "**/@typescript-eslint/parser": "^4.1.1"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/test/globals-test.ts"
    ]
  }
}
